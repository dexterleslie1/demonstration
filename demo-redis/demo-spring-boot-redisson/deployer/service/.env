# api 服务的 jvm 设置
java_opts=-Xmx512m

# redis 配置
# standalone、replication、sentinel、cluster
redis_mode=standalone
# standalone配置
standalone_address=redis://localhost:6379
standalone_password=123456
# replication配置
replication_addresses=redis://localhost:6479,redis://localhost:6480,redis://localhost:6481
# sentinel配置
sentinel_master_name=mymaster
sentinel_addresses=redis://localhost:26579,redis://localhost:26580,redis://localhost:26581
# cluster配置
cluster_addresses=redis://localhost:6679,redis://localhost:6680,redis://localhost:6681,redis://localhost:6682,redis://localhost:6683,redis://localhost:6684
